# Customer.io API Keys
# Get these from your Customer.io account dashboard
CUSTOMERIO_APP_API_KEY=your_app_api_key_here
CUSTOMERIO_CDP_API_KEY=your_cdp_api_key_here

# Customer.io Region (us or eu)
# Set this based on your Customer.io workspace region
CUSTOMERIO_REGION=us

# Server Configuration
# Port for the server (optional, defaults to 3000)
PORT=3000

# Environment
NODE_ENV=development

# Optional: Logging Configuration
LOG_LEVEL=info

# Optional: Security Configuration
# SESSION_SECRET=your_session_secret_here
# CORS_ORIGIN=http://localhost:3000

# Optional: Cache Configuration (if implementing Redis)
# REDIS_URL=redis://localhost:6379
# CACHE_TTL=300

# Optional: Database Configuration (if implementing database features)
# DATABASE_URL=postgresql://user:password@localhost:5432/subscription_center

# Optional: Monitoring Configuration
# SENTRY_DSN=your_sentry_dsn_here
# NEW_RELIC_LICENSE_KEY=your_new_relic_key_here 